* SELECCIONA EMPRESA DE TRABAJO
*
CLOSE DATABASES
SET BELL         ON
SET CENTURY      ON
SET CONFIRM      ON
SET DATE BRITISH
SET DELETE       ON
SET DEBUG        OFF
SET ESCAPE       OFF
SET EXCLUSIVE    OFF
SET HELP         OFF
SET MESSAGE TO 23
SET SAFETY       OFF
SET SCOREBOARD   OFF
SET STAT         OFF
SET SYSMENU      OFF
SET TALK         OFF
SET UNIQUE       OFF
SET FUNCTION 2 TO ' '
SET FUNCTION 8 TO ' '
SET HELP         OFF
PUBLIC XCIA,NLPP,PATCC,PATIP,PATVT,PATCP,PATCB,PATCH,PATOT,PATCM,XCOLOR,MES,XNIVEL,XTERMINAL,XFIRMA
XTERMINAL='12345678'
XNIVEL = 9
= INSMODE(.T.)
= NUMLOCK(.T.)
= CAPSLOCK(.T.)
DO PAS
   USE CONSE
   XCIA   = TRIM(CIA)
   PATIP  = TRIM(IP)
   PATVT  = TRIM(VT)
   PATCC  = TRIM(CC)
   PATCP  = TRIM(CP)
   PATCB  = TRIM(CB)
   PATCH  = TRIM(CH)
   PATCM  = TRIM(CM)
   NLPP   = NLPP
   XCOLOR = COLOR
   USE
   DO MENU2
CLOSE DATABASES
QUIT
RETURN

